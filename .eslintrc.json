{
  "env": {
    "browser": true,
    "jest": true
  },
  "extends": "airbnb",
  "settings": {
    "import/resolver": {
      "webpack": {
        "config": "config/webpack/development.js"
      }
    }
  },
  "rules": {
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ]
  }
}
